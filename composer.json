{
  "name": "fond-of-spryker/simple-router",
  "description": "Redirects and add locale in url if missing.",
  "license": "MIT",
  "authors": [
    {
      "name": "Julian Zimmermann",
      "email": "julian@jzit.de"
    }
  ],
  "require": {
    "php": ">=7.2",
    "jaybizzle/crawler-detect": "1.2.64",
    "sinergi/browser-detector": "6.1.4"
  },
  "require-dev": {
    "spryker/code-sniffer": "^0.17.14",
    "codeception/codeception": "^5.0.0",
    "mikey179/vfsstream": "^1.6",
    "phpro/grumphp": "^v1.14.0",
    "sebastian/phpcpd": "^6.0.0",
    "php-coveralls/php-coveralls": "^2.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "FondOfSpryker\\": "src/FondOfSpryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FondOfSpryker\\": "tests/FondOfSpryker/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "phpro/grumphp": true
    }
  }
}
